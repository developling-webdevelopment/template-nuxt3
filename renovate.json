{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:allNonMajor",
  ],
  "assignees": [
    "vanling"
  ],
   "packageRules": [
    {
      "matchPackagePatterns": ["commitlint","eslint","husky","lint-staged","prettier"],
      "groupName": "tools"
    },
     {
      "matchPackagePatterns": ["headlessui","iconify","nuxt","@nuxt","@nuxtjs","@vueuse","unplugin"],
      "groupName": "nuxtApplication"
    }
  ]
}
